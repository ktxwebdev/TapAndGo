<h1>City</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ document.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ document.name }}</td>
        </tr>
        <tr>
            <th>Lat</th>
            <td>{{ document.lat }}</td>
        </tr>
        <tr>
            <th>Long</th>
            <td>{{ document.long }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ document.status }}</td>
        </tr>
        <tr>
            <th>Stations</th>
            <td>{{ document.stations }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('city') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('city_edit', { 'id': document.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('city_delete', { 'id': document.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
